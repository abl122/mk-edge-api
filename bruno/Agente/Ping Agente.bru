meta {
  name: Ping Agente
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/agent/ping?tenant_id={{tenantId}}
  body: none
  auth: none
}

params:query {
  tenant_id: {{tenantId}}
}

docs {
  Verifica se o agente MK-Auth do tenant está acessível.
  
  Retorna:
  - agent: URL do agente
  - ok: true/false
}
