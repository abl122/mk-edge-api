get {
  url: {{baseUrl}}/api/me
  body: none
  auth: bearer
}

auth:bearer {
  token: {{authToken}}
}

docs {
  Obtém dados do usuário logado baseado no token JWT.
  
  Headers:
  - Authorization: Bearer {token}
  
  Retorna:
  - role=tenant: nome, cnpj, email, plano
  - role=admin: username, role
}
